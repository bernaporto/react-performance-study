    "module": "commonJs",
		"esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es2017",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "rootDir": ".",
